</div>


<script>
  $(document).ready(function () {
    $('#location').html(window.location.hostname)
      $("#contact").submit(function (e) {

          e.preventDefault(); // avoid to execute the actual submit of the form.

          var form = $(this);// agar es6 wale me karna h to $("#contact") use kar, varna ye timesaver accha h
          var url = '/contactUs';
          $("#submit").attr("disabled", "true");
          $("#submit").html("<span>Loading...</span>");

          $.ajax({
              type: "POST",
              url: url,
              data: form.serialize() + `&_id=<%- _id %>`,
              success: data => {
                console.log(data.message);
                  if (data.status == "error") {
                      showIt(".alert", data.message, "error");
                      $("#submit").removeAttr("disabled");
                      $("#submit").html("<span>Submit</span>");


                  } else {
                    showIt(".alert", data.message, "success");
                  }
              }
          }); 


      });


  });
</script>

